        // Initialize buttons and lightboxes on page load
        window.onload = function () {
            initializeButtons("buttons-container-1", 1, 4); // IDs from 1 to 3 for the first container
            initializeButtons("buttons-container-2", 5, 9); // IDs from 4 to 6 for the second container
            initializeButtons("buttons-container-3", 10, 13); // IDs from 7 to 9 for the second container
            initializeButtons("buttons-container-4", 14, 15); // IDs from 7 to 9 for the second container
            initializeButtons("buttons-container-5", 16, 18); // IDs from 7 to 9 for the second container
            initializeButtons("buttons-container-6", 19, 20); // IDs from 7 to 9 for the second container
            initializeButtons("buttons-container-7", 21, 23); // IDs from 7 to 9 for the second container
            initializeButtons("buttons-container-8", 24, 25); // IDs from 7 to 9 for the second container
            initializeButtons("buttons-container-9", 26, 30); // IDs from 7 to 9 for the second container
            initializeButtons("buttons-container-10", 31, 34); // IDs from 7 to 9 for the second container
            initializeButtons("buttons-container-11", 35, 38); // IDs from 7 to 9 for the second container
            initializeButtons("buttons-container-12", 39, 42); // IDs from 7 to 9 for the second container
            initializeButtons("buttons-container-13", 43, 44); // IDs from 7 to 9 for the second container
            initializeButtons("buttons-container-14", 45, 47); // IDs from 7 to 9 for the second container
            initializeButtons("buttons-container-15", 48, 50); // IDs from 7 to 9 for the second container
             initializeButtons("buttons-container-16", 51, 53); // IDs from 7 to 9 for the second container
            initializeLightboxes();
        };


        // Initialize buttons based on lightboxData
        function initializeButtons(containerId, startId, endId) {
            var buttonsContainer = document.getElementById(containerId);

            // Filter lightboxData to get items within the specified range
            var filteredData = lightboxData.filter(item => item.id >= startId && item.id <= endId);

            filteredData.forEach(item => {
                var button = createButton(item);
                buttonsContainer.appendChild(button);
            });
        }

        // Create a button for each item in lightboxData
        function createButton(item) {
            var button = document.createElement('button');
            button.textContent = `${item.heading}`;
            button.onclick = function() {
                showLightbox(item.id);
            };
            return button;
        }

        // Initialize lightboxes based on lightboxData
        function initializeLightboxes() {
            var lightboxesContainer = document.getElementById('lightboxes-container');

            lightboxData.forEach(item => {
                var lightbox = document.createElement('div');
                lightbox.classList.add('lightbox');
                lightbox.id = `lightbox-${item.id}`;

                var content = document.createElement('div');
                content.classList.add('lightbox-content');
                content.innerHTML = `
                    <span class="close-btn" onclick="closeLightbox(${item.id})">X</span>
                    <h2>${item.heading}</h2>
                    <div style="text-align: left; padding:20px;">
                    <p>${item.content}</p>
                    </div>
                `;

                lightbox.appendChild(content);
                lightboxesContainer.appendChild(lightbox);
            });
        }
        // Initialize lightboxes based on lightboxData
        function initializeLightboxes() {
            var lightboxesContainer = document.getElementById('lightboxes-container');

            lightboxData.forEach(item => {
                var lightbox = document.createElement('div');
                lightbox.classList.add('lightbox');
                lightbox.id = `lightbox-${item.id}`;

                var content = document.createElement('div');
                content.classList.add('lightbox-content');
                content.innerHTML = `
                    <span class="close-btn" onclick="closeLightbox(${item.id})">X</span>
                    <h2 style="text-align: right;">${item.heading}</h2>
                    <div style="text-align: right; padding:20px;">
                    <p>${item.content}</p>
                    </div>
                `;

                lightbox.appendChild(content);
                lightboxesContainer.appendChild(lightbox);
            });
        }

        // Show specific lightbox based on id
        function showLightbox(id) {
            var lightbox = document.getElementById(`lightbox-${id}`);
            lightbox.style.display = "flex";
        }

        // Close specific lightbox based on id
        function closeLightbox(id) {
            var lightbox = document.getElementById(`lightbox-${id}`);
            lightbox.style.display = "none";
        }
    </script>
</body>

